#include <iostream>
#include<math.h>
using namespace std;
int main() {
    // Write C++ code here
    int n;
    cin>>n;
    bool flag=0;
    for(int i=0;i<=30;i++){
        int ans=pow(2,i);
        if(ans==n)
            flag=1;
        
        
    }
    if (flag==0)
    cout<< "false";
    else
    cout<<"true";
    return 0;
    }
